        "default_icon":"logo.png"
    },
    "icons":{
        "128":"logo.png"
    },
    
      "permissions": [
        "tabs",
        "https://*/*",
        "http://*/*"
        
      ],
      "background":{
        "scripts":["./background.js","jquery-3.6.0.min.js"]
      },
      "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["contentscript.js"],
        "run_at": "document_end"    
        }
    ], 
      
